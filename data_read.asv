opt = detectImportOptions('data_clean.txt');
T = readtable('data_clean.txt', opt);


lat = T.Var2;
long = T.Var4;
alt = T.Var6;
speed = T.Var7;
temp1 = T.Var8;
press = T.Var9;
temp2 = T.Var10;
hum = T.Var11;


t = tiledlayout(1,1);
ax1 = axes(t);
plot(ax1,temp1,alt, '.b')
ax1.xco
xlabel("Teplota (°C)")
hold on
ax2 = axes(t);
plot(ax2, hum, alt, '.r')
ax2.XAxisLocation = 'top';
ax2.Color = 'none';
xlabel("Vlhkost (%)")
ylabel("Nadmořská výška (m)")
ax1.Box = 'on';
ax2.Box = 'on';
hold off